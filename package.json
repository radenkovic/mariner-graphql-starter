{
  "name": "mariner-graphql-starter",
  "version": "1.0.0",
  "description": "GraphQL Backend based on marinerjs",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon ./src --exec babel-node",
    "build": "babel src -d dist",
    "start": "yarn run migrate & node dist",
    "start:production": "NODE_ENV=production yarn run migrate && DEBUG=log* NODE_ENV=production node dist",
    "migrate": "knex migrate:latest",
    "lint": "eslint ./src --fix",
    "test": "jest --forceExit",
    "flow": "flow",
    "test:watch": "jest --watch --coverage",
    "test:coverage": "jest --coverage"
  },
  "author": "Dan Radenkovic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/radenkovic/mariner-graphql-starter"
  },
  "dependencies": {
    "apollo-server-express": "^2.0.2",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "graphql": "^0.13.2",
    "graphql-resolvers": "^0.2.2",
    "graphql-tools": "^3.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.4.0",
    "flow-bin": "^0.78.0",
    "jest": "^23",
    "prettier": "^1.10.2"
  }
}
